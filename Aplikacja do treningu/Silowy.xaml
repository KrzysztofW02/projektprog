<Window x:Class="Aplikacja_do_treningu.Silowy"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aplikacja_do_treningu"
        mc:Ignorable="d"
        Title="Silowy" Height="600" Width="1000">
    <Grid Background="#343434">
        <Label x:Name="silowy_latwy" Content="Łatwy" HorizontalAlignment="Left" Margin="98,412,0,0" VerticalAlignment="Top" Foreground="#FF11FF00" FontSize="48" MouseDown="silowy_latwy_MouseDown"/>
        <Label x:Name="silowy_normalny" Content="Normalny" HorizontalAlignment="Center" VerticalAlignment="Top" Foreground="#FFFF9300" FontSize="48" Margin="0,412,0,0" MouseDown="silowy_normalny_MouseDown"/>
        <Label x:Name="silowy_trudny" Content="Trudny" HorizontalAlignment="Left" Margin="760,412,0,0" VerticalAlignment="Top" Foreground="Red" FontSize="48" RenderTransformOrigin="1.827,0.58" MouseDown="silowy_trudny_MouseDown"/>
        <Label x:Name="Poziom_trudnosci" Content="Wybierz poziom trudności" HorizontalAlignment="Center" Margin="0,31,0,0" VerticalAlignment="Top" FontSize="48" Foreground="White"/>

    </Grid>
</Window>
